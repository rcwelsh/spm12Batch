    echo	 
    echo "                         Be sure to specify complex params inside of quotes."
    echo
    echo "                         do \"mcflirt --help\" for help."
    echo "                         do \"slicetimer --help\" for help."
    echo "                         do \"fslmaths --help\" for help."
    echo "                         default for realignfMRI is \"-cost normcorr -stats -plots\""
    echo "                         default for sliceTime is \"\""
