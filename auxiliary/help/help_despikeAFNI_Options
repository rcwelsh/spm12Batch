    echo "${helpComment} prepending name          : ${outputName}"
    echo "${helpComment} Functional Path          : ${fmriPATH}"
    echo "${helpComment} Volume Wildcard          : ${volumeWILD}"
    echo "${helpComment} subject output directory : ${SUBJDIROUT}"
    echo "${helpComment}"
    echo 

    . ${thisDir}/auxiliary/help/help_options_end
